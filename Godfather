public class Godfather extends Character{

    public Godfather(){
	hp = 100;
	strength = 8;
	defense = 7;
	attackRating = 6;
	type = "Mafia"
	    }

    public Godfather(String n){
	name = n;
	hp = 100;
	strength = 5;
	defense
	    }
 
    public String toString(){
	String s = super.toString();
	return s
	    }

 
